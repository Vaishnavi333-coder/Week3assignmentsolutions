---Posts.jsx---
import { useEffect, useState } from "react";

function Posts() {
  const [posts, setPosts] = useState([]);

  useEffect(() => {
    fetch("https://jsonplaceholder.typicode.com/posts")
      .then(res => res.json())
      .then(data => setPosts(data.slice(0, 10))) // Show only 10 posts
      .catch(err => console.error(err));
  }, []);

  return (
    <div style={{ padding: "20px" }}>
      <h2 style={{ textAlign: "center" }}>User Posts</h2>
      {posts.map(post => (
        <div key={post.id} style={{
          border: "1px solid #ccc",
          margin: "10px",
          padding: "10px",
          borderRadius: "8px"
        }}>
          <h4>{post.title}</h4>
          <p>{post.body}</p>
        </div>
      ))}
    </div>
  );
}

export default Posts;

----ProductDetails.jsx---
import { useParams, Link } from "react-router-dom";

function ProductDetails() {
  const { id } = useParams();

  const products = [
    { id: 1, name: "Laptop", price: 65000, description: "High performance laptop" },
    { id: 2, name: "Mobile", price: 25000, description: "Latest Android smartphone" },
    { id: 3, name: "Headphones", price: 4000, description: "Noise-cancelling headphones" },
    { id: 4, name: "Smartwatch", price: 8000, description: "Water-resistant smartwatch" },
  ];

  const product = products.find(p => p.id === parseInt(id));

  if (!product) return <h3>Product not found</h3>;

  return (
    <div style={{ textAlign: "center" }}>
      <h2>Product Details</h2>
      <div style={{
        border: "1px solid gray",
        display: "inline-block",
        padding: "20px",
        borderRadius: "10px"
      }}>
        <h3>{product.name}</h3>
        <p><b>Price:</b> ₹{product.price}</p>
        <p><b>Description:</b> {product.description}</p>
        <Link to="/Products">Back to Products</Link>
      </div>
    </div>
  );
}

export default ProductDetails;

----Products.jsx----
import { Link } from "react-router-dom";

function Products() {
  const products = [
    { id: 1, name: "Laptop", price: 65000, image: "https://cdn-dynmedia-1.microsoft.com/is/image/microsoftcorp/13-laptop-platinum-right-render-fy25:VP4-1260x795?fmt=png-alpha" },
    { id: 2, name: "Mobile", price: 25000, image: "https://image.made-in-china.com/2f0j00qZPiGaTHIVkU/Factory-Supply-S21-Mobile-Phone-Large-Memory-24-48MP-High-Resolution-Cellphone-7-2-Inch-Big-Screen-Dual-SIM-Card-Phone.webp" },
    { id: 3, name: "Headphones", price: 4000, image: "data:image/jpeg;base64," },
    { id: 4, name: "Smartwatch", price: 8000, image: "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTfDK7bX7nOQYeXl12ULxh8eJfzkXKpaRlybQ&s" },
  ];

  return (
    <div style={{ textAlign: "center" }}>
      <h2>Product List</h2>
      <div style={{
        display: "flex",
        justifyContent: "center",
        flexWrap: "wrap",
        gap: "20px"
      }}>
        {products.map(p => (
          <div key={p.id} style={{
            border: "1px solid #ccc",
            borderRadius: "10px",
            width: "200px",
            padding: "10px",
            backgroundColor: "#f9f9f9"
          }}>
            <img src={p.image} alt={p.name} width="150" height="150" />
            <h4>{p.name}</h4>
            <p>Price: ₹{p.price}</p>
            <Link to={`/ProductDetails/${p.id}`}>View Details</Link>
          </div>
        ))}
      </div>
    </div>
  );
}

export default Products;


